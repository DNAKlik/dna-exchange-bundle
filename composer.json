{
    "name": "dnaklik/dna-exchange-bundle",
    "description": "Happy dna exchange",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["symfony-ux"],
    "authors": [
        {
            "name": "Walter Mulder",
            "email": "info@dnaklik.nl"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/config": "^6.4",
        "symfony/dependency-injection": "^6.4",
        "symfony/http-kernel": "^6.4",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "symfony/console": "^6.4",
        "symfony/dotenv": "^6.4",
        "twig/twig": "^2.0",
        "symfony/twig-bundle": "^6.4",
        "symfony/security-bundle": "6.4.*",
        "symfony/asset": "^6.4",
        "symfony/asset-mapper": "^6.4",
        "symfony/yaml": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "DnaKlik\\DnaExchangeBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DnaKlik\\DnaExchangeBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.4",
        "symfony/framework-bundle": "^6.4",
        "symfony/browser-kit": "^6.4",
        "symfony/maker-bundle": "^1.50"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
